<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/titulo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Spinner
        android:id="@+id/sporigen"
        android:layout_width="156dp"
        android:layout_height="30dp"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/editTextText2"
        tools:layout_editor_absoluteX="-1dp" />

    <Spinner
        android:id="@+id/spdestino"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toEndOf="@+id/sporigen"
        app:layout_constraintTop_toBottomOf="@+id/editTextText3" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="155dp"
        android:layout_height="41dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Bien o servicio"
        app:layout_constraintTop_toBottomOf="@+id/editCantidad"
        tools:layout_editor_absoluteX="0dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="154dp"
        android:layout_height="45dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="20dp"
        android:text="Salir"
        app:layout_constraintStart_toEndOf="@+id/save"
        app:layout_constraintTop_toBottomOf="@+id/spdestino" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="155dp"
        android:layout_height="41dp"
        android:layout_marginTop="68dp"
        android:ems="10"
        android:inputType="text"
        android:text="Origen"
        app:layout_constraintTop_toBottomOf="@+id/spbienservicio"
        tools:layout_editor_absoluteX="0dp" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="171dp"
        android:layout_height="45dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="68dp"
        android:ems="10"
        android:inputType="text"
        android:text="Destino"
        app:layout_constraintStart_toEndOf="@+id/editTextText2"
        app:layout_constraintTop_toBottomOf="@+id/spproducto" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="171dp"
        android:layout_height="42dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Producto"
        app:layout_constraintStart_toEndOf="@+id/editTextText4"
        app:layout_constraintTop_toBottomOf="@+id/editCantidad" />

    <Button
        android:id="@+id/save"
        android:layout_width="168dp"
        android:layout_height="46dp"
        android:layout_marginTop="24dp"
        android:text="Guardar"
        app:layout_constraintTop_toBottomOf="@+id/sporigen"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/title"
        android:layout_width="379dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="text"
        android:text="Title"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTime" />

    <EditText
        android:id="@+id/editTextTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="time"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="195dp" />

    <EditText
        android:id="@+id/editCantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:hint="Cantidad"
        android:inputType="numberDecimal"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <Spinner
        android:id="@+id/spbienservicio"
        android:layout_width="156dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/editTextText4"
        tools:layout_editor_absoluteX="0dp" />

    <Spinner
        android:id="@+id/spproducto"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/spbienservicio"
        app:layout_constraintTop_toBottomOf="@+id/editTextText5" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="33dp"
        android:layout_height="38dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/creardb"
        android:layout_width="209dp"
        android:layout_height="47dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="8dp"
        android:text="crear db"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/save" />

    <TextView
        android:id="@+id/data"
        android:layout_width="308dp"
        android:layout_height="125dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="52dp"
        android:text="en espera de datos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/creardb" />

</androidx.constraintlayout.widget.ConstraintLayout>